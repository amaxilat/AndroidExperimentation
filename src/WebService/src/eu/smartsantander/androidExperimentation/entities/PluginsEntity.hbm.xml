<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="eu.smartsantander.androidExperimentation.entities.Plugin" table="plugins" schema=""
           catalog="androidExperimentation">
        <id name="id">
            <column name="id" sql-type="int" length="10" not-null="true"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar" length="256" not-null="true"/>
        </property>
        <property name="contextType">
            <column name="contextType" sql-type="varchar" length="256" not-null="true"/>
        </property>
        <property name="runtimeFactoryClass">
            <column name="runtimeFactoryClass" sql-type="varchar" length="256" not-null="true"/>
        </property>
        <property name="description">
            <column name="description" sql-type="varchar" length="256" not-null="true"/>
        </property>
        <property name="installUrl">
            <column name="installUrl" sql-type="varchar" length="256" not-null="true"/>
        </property>
        <property name="filename">
            <column name="filename" sql-type="varchar" length="256" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>